      "id": "python3",
      "name": "Python 3",
      "description": "Run Python 3 script",
      "language": "Python",
      "extensions": ["py", "pyw"],
      "command": "python3",
      "args": ["${file}"],
      "workingDirectory": "${fileDir}",
      "env": {}
    },
    {
      "id": "python2",
